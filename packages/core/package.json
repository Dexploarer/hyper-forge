{
  "name": "@asset-forge/core",
  "version": "1.0.0",
  "description": "AI-powered 3D asset generation system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:frontend\" \"bun run dev:backend\"",
    "dev:all": "concurrently \"bun run dev:frontend\" \"bun run dev:backend\" \"bun run dev:workers\"",
    "dev:frontend": "vite",
    "dev:backend": "bun server/api-elysia.ts",
    "dev:workers": "bun server/workers/start-workers.ts",
    "dev:worker:single": "bun server/workers/generation-worker.ts",
    "build": "bun run clean && vite build",
    "build:services": "bun scripts/build-services.mjs",
    "start": "bun run build && bun run start:prod",
    "start:prod": "concurrently \"bun run start:api\" \"bun run start:workers\"",
    "start:api": "bun run db:migrate && NODE_ENV=production bun server/api-elysia.ts",
    "start:workers": "NODE_ENV=production bun server/workers/start-workers.ts",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "preview": "vite preview",
    "assets:audit": "bunx tsx scripts/audit-assets.ts",
    "assets:normalize": "bunx tsx scripts/normalize-all-assets.ts",
    "check:deps": "depcheck",
    "check:all": "knip",
    "count:lines": "bun scripts/count-lines.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "bun test __tests__/integration __tests__/unit",
    "test:watch": "bun test __tests__/integration __tests__/unit --watch",
    "test:coverage": "bun test __tests__/integration __tests__/unit --coverage",
    "test:integration": "bun test __tests__/integration",
    "test:unit": "bun test __tests__/unit",
    "test:load": "bun test __tests__/integration/load.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "bun test __tests__/integration __tests__/unit && playwright test",
    "test:advanced": "bun test __tests__/advanced",
    "test:contract": "bun test __tests__/advanced/contract-testing",
    "test:property": "bun test __tests__/advanced/property-based",
    "test:performance": "bun test __tests__/advanced/performance",
    "test:chaos": "bun test __tests__/advanced/chaos",
    "test:mutation": "bunx stryker run",
    "test:mutation:report": "open reports/mutation/index.html",
    "test:perf": "bun scripts/test-performance.ts",
    "test:perf:baseline": "bun scripts/test-performance.ts baseline",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bun run server/db/migrate.ts",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio"
  },
  "keywords": [
    "ai",
    "3d",
    "generation",
    "meshy",
    "gpt-4",
    "rpg",
    "game-assets"
  ],
  "author": "Lucid",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.42",
    "@ai-sdk/google": "^2.0.29",
    "@ai-sdk/openai": "^2.0.64",
    "@elevenlabs/elevenlabs-js": "^2.22.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/cron": "^1.4.1",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/server-timing": "^1.4.0",
    "@elysiajs/static": "^1.4.6",
    "@elysiajs/swagger": "^1.3.1",
    "@mediapipe/hands": "^0.4.1675469240",
    "@pixiv/three-vrm": "^3.4.4",
    "@privy-io/react-auth": "^3.6.1",
    "@privy-io/server-auth": "^1.32.5",
    "@qdrant/js-client-rest": "^1.15.1",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@sentry/bun": "^10.25.0",
    "@solana-program/memo": "^0.10.0",
    "@solana-program/system": "^0.10.0",
    "@solana-program/token": "^0.8.0",
    "@solana/kit": "^5.0.0",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.0.0",
    "@tensorflow-models/hand-pose-detection": "^2.0.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@xyflow/react": "^12.9.2",
    "ai": "^5.0.89",
    "chalk": "^5.6.2",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "elysia": "^1.4.15",
    "elysia-prometheus": "^1.0.0",
    "elysia-rate-limit": "^4.4.2",
    "elysiajs-cdn-cache": "^0.0.13",
    "express": "^5.1.0",
    "immer": "^10.2.0",
    "lucide-react": "^0.553.0",
    "node-fetch": "^3.3.2",
    "opossum": "^9.0.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "postgres": "^3.4.5",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-joyride": "^2.9.3",
    "recharts": "^3.3.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "three": "^0.181.0",
    "three-mesh-bvh": "^0.9.2",
    "three-stdlib": "^2.36.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^16.0.2",
    "@playwright/test": "^1.56.1",
    "@stryker-mutator/core": "^9.3.0",
    "@stryker-mutator/typescript-checker": "^9.3.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "1.3.2",
    "@types/node": "^24.10.0",
    "@types/opossum": "^8.1.9",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.181.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fast-check": "^4.3.0",
    "happy-dom": "^20.0.10",
    "knip": "^5.68.0",
    "msw": "^2.12.1",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22.12.0"
  },
  "packageManager": "bun@1.3.2",
  "private": false
}
