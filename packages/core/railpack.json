{
  "$schema": "https://railpack.com/schema.json",
  "packages": {
    "bun": "latest",
    "node": "22.12.0"
  },
  "caches": {
    "node_modules": {
      "key": "node_modules",
      "paths": ["node_modules"]
    }
  },
  "deploy": {
    "inputs": [
      {
        "step": "mise",
        "comment": "Ensures bun and node are available at runtime"
      }
    ],
    "cmd": "NODE_ENV=production bun packages/core/server/workers/start-workers.ts"
  }
}
